<!DOCTYPE html>
<html>
<head>
  <title>Packing</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if current_user.present? %>  
    <%= link_to('Logout', destroy_user_session_path, :method => :delete) %>
  <% end %>
<%= yield %>

</body>
</html>
